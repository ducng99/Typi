{
  "name": "typi-frontend",
  "description": "Typi's frontend app. Made with Vue.js",
  "version": "1.0.0",
  "private": false,
  "license": "AGPL-3.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.5",
    "idle-vue": "^2.0.5",
    "incstr": "^1.2.3",
    "node-rsa": "^1.1.1",
    "vue": "^2.6.11",
    "vue-clickaway": "^2.2.2",
    "vue-cookies": "^1.7.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-empty": "off",
      "no-unused-labels": "off",
      "vue/no-unused-components": [
        "error",
        {
          "ignoreWhenBindingPresent": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
